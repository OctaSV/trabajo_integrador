        // ES6 Modules or TypeScript
        import Swal from 'sweetalert2'
        // CommonJS
        const Swal = require('sweetalert2')
        Swal.fire({
            icon: 'error',
            title: 'Oops... no tiene ninguna canción añadida a su playlist',
            text: 'Algo va mal!',
            footer: '<a href="./index.html">HOME</a>'
          })







